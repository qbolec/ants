  width:100%;
  height:100%;
}
.double_page.up_down{
  position: relative;
  overflow:hidden;
}
.double_page.up_down .page_a_container,
.double_page.up_down .page_b_container{
  position: absolute;
  transition:top 1s;
}
.double_page.up_down.side_page_b .page_b_container,
.double_page.up_down.side_page_a .page_a_container{
  top:0%;
}
.double_page.up_down.side_page_b .page_a_container{
  top:-100%;
}
.double_page.up_down.side_page_a .page_b_container{
  top:100%;
}
@-webkit-keyframes doublePageFlipOutLeft {
    0% {
        -webkit-transform: perspective(1000px) rotateY(0deg);
        opacity: 1;
    }

    50% {
        -webkit-transform: perspective(1000px) rotateY(-90deg);
        opacity: 1;
    }
    100% {
        -webkit-transform: perspective(1000px) rotateY(-90deg);
        opacity: 0;
    }
}
@-webkit-keyframes doublePageFlipInLeft {
    0% {
        -webkit-transform: perspective(1000px) rotateY(90deg);
        opacity: 0;
    }

    50% {
        -webkit-transform: perspective(1000px) rotateY(90deg);
        opacity: 1;
    }
    100% {
        -webkit-transform: perspective(1000px) rotateY(0deg);
        opacity: 1;
    }
}

@-webkit-keyframes doublePageFlipOutRight {
    0% {
        -webkit-transform: perspective(1000px) rotateY(0deg);
        opacity: 1;
    }

    50% {
        -webkit-transform: perspective(1000px) rotateY(90deg);
        opacity: 1;
    }
    100% {
        -webkit-transform: perspective(1000px) rotateY(90deg);
        opacity: 0;
    }
}
@-webkit-keyframes doublePageFlipInRight {
    0% {
        -webkit-transform: perspective(1000px) rotateY(-90deg);
        opacity: 0;
    }

    50% {
        -webkit-transform: perspective(1000px) rotateY(-90deg);
        opacity: 1;
    }
    100% {
        -webkit-transform: perspective(1000px) rotateY(0deg);
        opacity: 1;
    }
}


