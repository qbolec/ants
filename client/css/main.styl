@-webkit-keyframes glowLoop {
    0% {
      text-shadow: 0 0 17px #FFF;
      color:#CCC;
    }

    50% {
      text-shadow: 0 0 2px #FFF;
      color:#FFF;
    }
    100% {
      text-shadow: 0 0 7px #FFF;
      color:#CCC;
    }
}
