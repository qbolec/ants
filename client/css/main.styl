}
.welcome_page .elevator_speech{
  background: black;
}
.welcome_page .elevator_speech b{
  -webkit-animation: glowLoop 1s linear infinite;
  -webkit-animation-direction: alternate;
}

@-webkit-keyframes tutorialGlowLoop {
    0% {
      background-color: #3276b1;
    }
    50% {
      background-color: #FFF;
    }
    100% {
      background-color: #428bca;
    }
}
.introjs-highlight.introjs-showElement{
  -webkit-animation: tutorialGlowLoop 3s linear infinite;
  -webkit-animation-direction: alternate;
}


