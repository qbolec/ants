<span class="id"><%- d.skill.id %></span> <span class="name"><%- d.skill.name %></span>
<ul>
<% _.each(d.materials_ids, function(material_id){ %>
  <li><%- material_id %></li>
<% }); %>
</ul>
<div>
  <span class="into_buckets"><%- Texts.get('into_buckets/label') %></span>
  <% for(var i=1;i<=3;++i){ %>
  <button class="btn btn-default into_bucket bucket-<%- i %>">#<%- i %></button>
  <% } %>
</div>
