<form role="form">
  <div class="form-group">
    <p><%- Texts.get('confirm_email/question',d.email) %>
  </div>
  <div class="form-group">
    <input type="button" value="<%- Texts.get('confirm_email/confirm/label') %>" class="confirm btn btn-primary btn-lg">
  </div>
  <div class="form-group">
    <p><%- Texts.get('confirm_email/cancel',d.email) %>
  </div>
</form>
